{
  "name": "telecom-bundle",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "8.9.3",
    "npm": "5.0.3"
  },
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "production": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
    "webhook": "./sendgrid_webhook.sh",
    "client": "npm run start --prefix client",
    "mocha": "NODE_ENV=test ./node_modules/mocha/bin/mocha",
    "lint": "eslint server/* test/**/* index.js",
    "coverage": "NODE_ENV=test istanbul cover --print both _mocha -- -R spec",
    "test": "npm run lint && npm run coverage",
    "debug": "node --debug-brk index.js",
    "debugtest": "NODE_ENV=test ./node_modules/mocha/bin/mocha --recursive --debug-brk"
  },
  "repository": {
    "type": "git"
  },
  "author": "Alex Roberto CorrÃªa",
  "license": "ISC",
  "homepage": "https://github.com/AlexRobertoCorrea/telecom-bundle",
  "dependencies": {
    "body-parser": "1.18.2",
    "compression": "1.6.x",
    "concurrently": "3.5.0",
    "express": "4.16.2",
    "nconf": "0.8.5",
    "node-all-paths": "1.0.3",
    "nodemon": "1.11.0",
    "winston": "2.4.0"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "eslint": "4.13.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.4.0",
    "istanbul": "0.4.5",
    "mocha": "3.2.0",
    "sinon": "4.2.1",
    "supertest": "3.0.0"
  }
}
